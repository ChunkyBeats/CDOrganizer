
<% @cds.each() do |cd| %>
<form action="/artist" method="post" class="form">
  <label for="artist"><a href="/collection/<%=cd.id() %>"><%= cd.title() %></a></li></label>
  <input id="yes" name="yes" type="radio" value= <%= cd.title() %>> Yes
  <input id="no" name="no" type="radio" value= <%= cd.title() %>> No
  <p></p>
    <% end %>

  <button type="submit">Reset</button>
</form>
